//java program
//Fahrenheit to Celsius 
import java.util.*;
class Celsius{
    
    public static void main (String args[]){
        Scanner sc=new Scanner(System.in);
        float F, C;
        System.out.print("Enter Fahrenheit: ");
        F=sc.nextFloat();
        C = ((F-32)*5)/9;
        System.out.println("Temperature in celsius is: "+C);
   }
}
